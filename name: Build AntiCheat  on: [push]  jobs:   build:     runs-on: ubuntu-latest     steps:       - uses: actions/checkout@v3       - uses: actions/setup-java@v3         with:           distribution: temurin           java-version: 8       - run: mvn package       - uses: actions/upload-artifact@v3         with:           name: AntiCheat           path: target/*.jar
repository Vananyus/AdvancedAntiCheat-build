name: Build AntiCheat

on: [push]

jobs:
  build:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v3
      - uses: actions/setup-java@v3
        with:
          distribution: temurin
          java-version: 8
      - run: mvn package
      - uses: actions/upload-artifact@v3
        with:
          name: AntiCheat
          path: target/*.jar
